        //     ],
        //     "voby/jsx-runtime/*": [
        //         "../voby/dist/jsx/*"
        //     ],
        // },
        // "typeRoots": [
        //     "../voby/src",
        //     "voby"
        // ],
        // "types": [
        //     "jsx"
        // ]
    },
